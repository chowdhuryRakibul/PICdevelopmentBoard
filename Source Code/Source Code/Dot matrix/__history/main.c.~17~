#include <main.h>
#define disp1 PIN_A2
#define disp2 PIN_A1
#define disp3 PIN_A0
#define disp4 PIN_A3
int row[8] = {PIN_C1,PIN_D4,PIN_D1,PIN_D6,PIN_A0,PIN_D0,PIN_A1,PIN_A4};
int col[8] = {PIN_D5,PIN_A2,PIN_A3,PIN_C0,PIN_A5,PIN_D7,PIN_D3,PIN_D2};

void main()
{
  
   while(TRUE)
   {
      output_low(col[2]);
      for(int i = 0;i<8;i++)
      {
         output_high(row[i]);
         delay_ms(1000);
      }
      
      
   }

}
